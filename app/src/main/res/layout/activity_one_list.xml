<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".OneList">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="To Be Completed"
        android:id="@+id/textView6" />

    <fragment
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:id="@+id/tbd_list"
        android:name="cs4720.cs.virginia.edu.checklist.OneListFragment"
        tools:layout="@layout/listfragment_main" android:layout_width="match_parent"
        android:layout_height="267dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Completed"
        android:id="@+id/textView7" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:name="cs4720.cs.virginia.edu.checklist.CompListFragment"
        tools:layout="@layout/listfragment_main"
        android:id="@+id/completed_list" />


</LinearLayout>
