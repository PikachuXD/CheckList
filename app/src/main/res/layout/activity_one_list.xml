<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".OneList"
    android:id="@+id/OneList">


    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:name="cs4720.cs.virginia.edu.checklist.ToBeCompleteHeader"
        android:id="@+id/fragment"
        tools:layout="@layout/activity_to_be_complete_header" />

    <fragment
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:id="@+id/tbd_list"
        android:name="cs4720.cs.virginia.edu.checklist.OneListFragment"
        tools:layout="@layout/listfragment_main" android:layout_width="match_parent"
        android:layout_height="267dp" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:name="cs4720.cs.virginia.edu.checklist.CompletedListHeaderFragment"
        android:id="@+id/fragment3"
        tools:layout="@layout/fragment_completed_list_header"/>

    <fragment
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:name="cs4720.cs.virginia.edu.checklist.CompListFragment"
        tools:layout="@layout/listfragment_main"
        android:id="@+id/completed_list" />


</LinearLayout>
